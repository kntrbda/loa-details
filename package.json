{
  "name": "loa-details",
  "version": "1.3.1",
  "description": "Damage meter for Lost Ark",
  "productName": "LOA Details",
  "author": "Herysia, Mathicha, Eren Kara <erenkyi0@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ && npm run typecheck",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron",
    "build": "quasar build -m electron",
    "release": "npm run lint && quasar build -m electron -P always",
    "install": "electron-rebuild"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "meter-core": "github:lost-ark-dev/meter-core"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@quasar/app-vite": "^1.4.3",
    "@quasar/extras": "^1.16.4",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "electron": "^24",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "html2canvas": "^1.4.1",
    "pinia": "^2.1.3",
    "prettier": "^2.8.8",
    "quasar": "^2.12.0",
    "typescript": "^4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": "^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
